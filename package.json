{
  "name": "@aduyng/jwt-knex",
  "version": "0.0.2",
  "description": "JWT with Knex as the storage",
  "license": "MIT",
  "repository": "https://github.com/aduyng/jwt-knex",
  "main": "src/index.js",
  "author": {
    "name": "Duy Nguyen",
    "email": "aduyng@gmail.com",
    "url": "https://github.com/aduyng"
  },
  "files": [
    "src"
  ],
  "scripts": {
    "test": "jest --watchAll --runInBand --detectOpenHandles",
    "coverage": "npm test -- --coverage",
    "lint": "eslint .",
    "prerelease": "npm run lint",
    "release": "standard-version",
    "postpublish": "git push origin master --follow-tags",
    "knex": "knex --knexfile=knexfile.js",
    "oracle:bash": "docker exec -it oracle-xe-11g-r2 /bin/bash",
    "bash": "docker exec -it jwt-knex /bin/bash"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "keywords": [
    "jwt",
    "oracle"
  ],
  "peerDependencies": {
    "knex": "^0.20.14",
    "oracledb": "^4.2.0"
  },
  "dependencies": {
    "jsonwebtoken": "^8.5.1",
    "knex": "0.20.14",
    "oracledb": "4.2.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.1.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "standard-version": "^7.1.0"
  }
}
